	<queryString language="SQL">
		<![CDATA[SELECT oildb.money_book.date,
	oildb.money_book.dscription,
	oildb.money_book.amount,
	oildb.deal_type.type
FROM oildb.money_book
	INNER JOIN oildb.deal_type ON 
	 oildb.money_book.deal_type = oildb.deal_type.id 
WHERE 
	 oildb.money_book.date BETWEEN $P{dateFrom} AND $P{dateTo} 
	 AND oildb.money_book.deal_type = $P{dealType}]]>
